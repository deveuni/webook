<?xml version="1.0" encoding="UTF-8"?>

<!-- DTD지정  -->
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace : mapper를 외부에서 찾아오기 위한 속성 -->
<mapper namespace="com.webook.mapper.MemberMapper">

<!-- 회원가입 -->
<insert id="signup">
	insert into tbl_member(userId,userPass,userName,userPhon,userEmail,userAddr1,userAddr2,postcode,reg_date) 
	values (#{userId},#{userPass},#{userName},#{userPhon},#{userEmail},#{userAddr1},#{userAddr2},#{postcode},now())
</insert>







</mapper>  
  
  
  